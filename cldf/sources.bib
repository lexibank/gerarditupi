    Lgcode = {Mundurukú [mund1330]},
    Macro_area = {South America},
    Number = {4},
    Pages = {411–438},
    Src = {haspelmath},
    Title = {A Reconstruction of Nasal Harmony in Proto-Mundurukú (Tupi).},
